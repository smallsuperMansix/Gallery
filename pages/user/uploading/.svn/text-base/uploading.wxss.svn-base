/* 导入模板wxss */
@import "../../public/uploadItem/uploadItem.wxss";

/* pages/user/uploading/uploading.wxss */
.baseView{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 10rpx;
}
.upload{
  width: 60rpx;
  height: 60rpx;
}
.with_arrow {
  width: 710rpx;
  height: 1rpx;
  background: #d4d6da;
  margin: 20rpx;
}
.bodyView{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.text{
  width: 100%;
  height: 40rpx;
  text-align: left;
  font-size: 36rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
.img{
  width: 200rpx;
  height: 200rpx;
  margin: 10rpx;
}