<!-- 导入模板 -->
<import src="../../public/collect/collectItem.wxml" />
<import src="../../public/pagestate/pagestate.wxml" />
<template is="pagestate" data="{{...pageing}}" />
<!--pages/user/collect/collect.wxml-->
<view class='bodyView'>
  <view class='row-container'>
    <view class='content-wrapper'>
      <block wx:for="{{scList}}" wx:for-item="at" wx:key="id">
        <!-- 使用模板，传入当前循环对象item -->
        <template is="collectItem" data="{{...at,...{imgurl: imgbaseurl+'?filename='+at.filename} }}" />
      </block>
    </view>
  </view>
</view>